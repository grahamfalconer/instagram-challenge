<h1>Sign-up</h1>

<%= form_with scope: :user, url: users_path,  local: true do |form| %>
  <p> 
    <%= form.label :username %>
    <%= form.text_field :username %>
  </p>
  <p>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </p>
  <p>
    <%= form.submit 'Sign up' %>
  </p>
<% end %>



